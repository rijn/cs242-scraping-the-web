{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": [ "." ],
        "exclude": [
            "test",
            "node_modules",
            "coverage",
            "semantic"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "template": "./node_modules/minami"
    }
}
