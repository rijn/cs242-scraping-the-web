{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": [ "." ],
        "exclude": [
            "test",
            "test_manual",
            "node_modules",
            "coverage",
            "screenshots"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "destination": "./doc/",
        "encoding": "utf8",
        "recurse": true,
        "readme": "README.md",
        "template": "./node_modules/minami"
    }
}
